{
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "dependencies"
  ],
  "rules": {
    "comma-dangle": [
      "error",
      {
        "arrays": "only-multiline",
        "objects": "only-multiline"
      }
    ],
    "dependencies/no-cycles": 1,
    "no-param-reassign": [
      "error",
      {
        "props": true
      }
    ]
  }
}
