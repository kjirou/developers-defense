  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kjirou/developers-defense.git"
  },
  "scripts": {
    "b": "npm run build",
    "build": "$(npm bin)/gulp build",
    "d": "npm run develop",
    "develop": "$(npm bin)/gulp develop",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "jsdoc": "npm run jsdoc:generate && npm run jsdoc:open",
    "jsdoc:generate": "$(npm bin)/jsdoc -c ./jsdoc/conf.json -r -d ./jsdoc/out",
    "jsdoc:open": "open ./jsdoc/out/index.html",
    "lint": "$(npm bin)/eslint --ignore-pattern 'src/decls/' --ignore-pattern 'src/types/' '{src,test}/**/*.js'",
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('You must use Yarn to install, not NPM');\"",
    "serve": "$(npm bin)/gulp serve",
    "test": "npm run test:fastest",
    "test:fastest": "$(npm bin)/mocha --recursive",
    "test:power-assert": "$(npm bin)/mocha --require ./enable-power-assert.js --recursive"
  }
}
